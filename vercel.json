{
  "version": 2,
  "builds": [
    {
      "src": "dist/api/serverless.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/api/serverless.js"
    },
    {
      "src": "/health",
      "dest": "dist/api/serverless.js"
    },
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|svg|json|webmanifest))",
      "dest": "dist/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "dist/api/serverless.js"
    }
  ],
  "outputDirectory": "dist"
}
